<%= semantic_form_for @skill do |f| %>
  <%= f.inputs do %>
    <%= f.input :sport_id %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :ability_category %>
    <%= f.input :ability_level %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>

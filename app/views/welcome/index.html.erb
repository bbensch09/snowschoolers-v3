    <div class="section-box-container">
        <div>
            <p id="search-index-header" class="welcome-subheader navy-text">Already have a favorite mountain? See current deals on lessons, lift tickets, and more.<br>
            <span class="smaller-text"></span>
            <%= render partial: 'products/search' %>
            </p>
        </div>
    </div>
</section>



<%= semantic_form_for @rental do |f| %>
  <%= f.inputs do %>
    <%= f.input :lesson_id %>
    <%= f.input :student_id %>
    <%= f.input :resource_type %>
    <%= f.input :resource_id %>
    <%= f.input :status %>
    <%= f.input :other %>
  <% end %>

  <%= render partial: 'resources/eligible_equipment', locals: {resources: @resources}  %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
